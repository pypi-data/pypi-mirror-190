<odoo>
  <data noupdate="1">
    <record id="account_journal_vh_consumptions_sequence" model="ir.sequence">
      <field name="name">VH Consumptions Sequence</field>
      <field name="implementation">standard</field>
      <field name="prefix">%(range_year)s/C</field>
      <field name="padding">5</field>
      <field name="number_increment">1</field>
      <field name="use_date_range">True</field>
      <field name="active">True</field>
    </record>
    <record id="account_journal_vh_invoices_sequence" model="ir.sequence">
      <field name="name">VH Invoices Sequence</field>
      <field name="implementation">standard</field>
      <field name="prefix">%(range_year)s/F</field>
      <field name="padding">5</field>
      <field name="number_increment">1</field>
      <field name="use_date_range">True</field>
      <field name="active">True</field>
    </record>
    <record id="account_journal_vh_receipts_sequence" model="ir.sequence">
      <field name="name">VH Receipts Sequence</field>
      <field name="implementation">standard</field>
      <field name="prefix">%(range_year)s/R</field>
      <field name="padding">5</field>
      <field name="number_increment">1</field>
      <field name="use_date_range">True</field>
      <field name="active">True</field>
    </record>
  </data>
</odoo>