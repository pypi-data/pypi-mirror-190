<odoo>
  <data>
    <record id="view_vh_contract_template_form" model="ir.ui.view">
      <field name="name">vh.contract.template.form.inherit</field>
      <field name="model">contract.template</field>
      <field name="type">form</field>
      <field name="inherit_id" ref="contract.contract_template_form_view"/>
      <field name="arch" type="xml">
        <xpath expr="//field[@name='journal_id']" position="after">
          <field name="account_receivable_id"/>
        </xpath>
      </field>
    </record>
  </data>
</odoo>