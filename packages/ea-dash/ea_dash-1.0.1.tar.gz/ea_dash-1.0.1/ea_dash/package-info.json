{
  "name": "ea_dash",
  "version": "1.0.1",
  "description": "Dash versions of shared react component for Energy Aspects apps",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/energyaspects/ea-design-system.git"
  },
  "bugs": {
    "url": "https://github.com/energyaspects/ea-design-system/issues"
  },
  "url": "https://github.com/energyaspects/ea-design-system#readme",
  "main": "build/index.js",
  "scripts": {
    "start": "webpack-serve --config ./webpack.serve.config.js --open",
    "validate-init": "python _validate_init.py",
    "prepublishOnly": "npm run validate-init",
    "build:js": "webpack --mode production",
    "build:backends": "dash-generate-components ./src/lib/components ea_dash -p package-info.json --r-prefix 'ea_dc' --jl-prefix 'ea_dc' --ignore \\.test\\.",
    "build:backends-activated": "(. venv/bin/activate || venv\\scripts\\activate && npm run build:py_and_r)",
    "build": "npm run build:js && npm run build:backends",
    "build:activated": "npm run build:js && npm run build:backends-activated"
  },
  "author": "Pavol Soltes <soltes.pavol@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@energyaspects/ea-design-system-react": "*",
    "ramda": "^0.26.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "@plotly/dash-component-plugins": "^1.2.0",
    "@plotly/webpack-dash-dynamic-import": "^1.2.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "copyfiles": "^2.1.1",
    "css-loader": "^3.0.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-react": "^7.14.2",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-docgen": "^4.1.1",
    "react-dom": "^18.2.0",
    "style-loader": "^0.23.1",
    "styled-jsx": "^3.2.1",
    "terser-webpack-plugin": "^2.3.0"
  },
  "engines": {
    "node": ">=8.11.0",
    "npm": ">=6.1.0"
  }
}
